{
  "name": "lottonew-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "buildgo-watch-win": "npm-watch buildgo-win",
    "Go-build-win": "cd src/srv/cmd && go build -o ../../../dist/windows/lottonumbers.exe && cd ../../../",
    "Go-build-mac": "cd src/srv/cmd && cross-env GOOS=darwin cross-env GOARCH=amd64 go build -o ../../../dist/mac/lottonumbers.app && cd ../../../",
    "Go-build-linux": "cd src/srv/cmd && cross-env GOOS=linux cross-env GOARCH=amd64 go build -o ../../../dist/linux/lottonumbers && cd ../../../",
    "Go-build-all": "npm-run-all -p -l Go-build-win Go-build-mac Go-build-linux",
    "Go-run-win": "cd src/srv/cmd && go run main.go --web --game=monday-lotto",
    "ginkgo-test-watch": "cd ./src/srv &  ginkgo watch -r --v",
    "ginkgo-models-test": "cd ./src/srv & ginkgo -r ./models --v"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "6.0.0-alpha.11",
    "@mui/styled-engine-sc": "6.0.0-alpha.18",
    "@pigment-css/react": "0.0.20",
    "@tanstack/react-router": "^1.47.1",
    "autoprefixer": "^10.4.20",
    "flowbite": "^2.5.1",
    "flowbite-react": "^0.10.1",
    "jsx-control-statements": "^3.2.8",
    "postcss": "^8.4.41",
    "react": "19.0.0-rc-9d2da591-20240808",
    "react-dom": "19.0.0-rc-9d2da591-20240808",
    "styled-components": "^6.1.12",
    "tailwindcss": "^3.4.9"
  },
  "devDependencies": {
    "@pigment-css/vite-plugin": "0.0.20",
    "@tanstack/router-devtools": "^1.47.1",
    "@tanstack/router-plugin": "^1.47.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "npm-run-all": "^4.1.5",
    "vite": "^5.4.0",
    "vite-plugin-react-control-statements": "^1.0.1"
  }
}
