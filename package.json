{
    "name": "lottonumbers",
    "version": "1.0.0",
    "description": "",
    "main": "appmain.js",
    "watch": {
        "buildgo": {
            "patterns": [
                "../server"
            ],
            "extensions": "go",
            "quiet": false
        }
    },
    "scripts": {
        "spa-build": "cross-env NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=production webpack --mode production",
        "spa-builddev": "cross-env NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=development webpack --mode development",
        "spa-hotdev": "cross-env NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider webpack serve --progress --mode development",
        "buildgo-watch-win": "npm-watch buildgo-win",
        "Go-build-win": "cd src/srv/cmd && go build -o ../../../build/windows/lottonumbers.exe && cd ../../../",
        "Go-build-mac": "cd src/srv/cmd && cross-env GOOS=darwin cross-env GOARCH=amd64 go build -o ../../../build/mac/lottonumbers.app && cd ../../../",
        "Go-build-linux": "cd src/srv/cmd && cross-env GOOS=linux cross-env GOARCH=amd64 go build -o ../../../build/linux/lottonumbers && cd ../../../",
        "Go-build-all": "npm-run-all -p -l Go-build-win Go-build-mac Go-build-linux",
        "Go-run-win": "cd src/srv/cmd && go run main.go --web",
        "ginkgo-test-watch": "cd ./src/srv &  ginkgo watch -r --v",
        "ginkgo-models-test": "cd ./src/srv & ginkgo -r ./models --v"
    },
    "oldscripts": {
        "Go:buildAndrun:win": "npm-run-all -s -l Go:build:win Go:run:win",
        "buildgo:watch:win": "npm-watch buildgo:win",
        "buildgo:watch:linux": "npm-watch buildgo:linux",
        "buildgo:win": "cd src/srv/cmd && go build -o ../../../build/windows/lottonumbers.exe && cd ../../../",
        "buildgo:mac": "cd src/srv/cmd && cross-env GOOS=darwin cross-env GOARCH=amd64 go build -o ../../../build/mac/lottonumbers.app && cd ../../../",
        "buildgo:linux": "cd src/srv/cmd && cross-env GOOS=linux cross-env GOARCH=amd64 go build -o ../../../build/linux/lottonumbers && cd ../../../",
        "buildgoall": "npm-run-all -p -l buildgo:*",
        "spa:build": "cross-env NODE_ENV=production webpack --mode production",
        "spa:builddev": "cross-env NODE_ENV=development webpack --mode development",
        "spa:hotdev": "npm run dev",
        "test": "jest --verbose",
        "test:watch": "jest --verbose --watch",
        "dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --progress --mode development",
        "runserver:win": "npm-run build/windows/lottonumbers.exe --web",
        "runserver:linux": "npm-run build/linux/lottonumbers --web",
        "server:win": "npm-run-all -s -l buildgo:win runserver:win",
        "server:linux": "npm-run-all -s -l buildgo:linux runserver:linux",
        "hotdevall:win": "run-p runserver:win hotdev",
        "hotdevall:linux": "run-p runserver:linux hotdev",
        "ginkgo:test:watch": "cd ./src/srv &  ginkgo watch -r --v",
        "ginkgo:models:test": "cd ./src/srv & ginkgo -r ./models --v",
        "go-test:handlers": "cd ./src/zdapiserver/webserver & env-cmd -f ../../../.env go test",
        "go-test:helpers": "cd ./src/zdapiserver/models & env-cmd -f ../../../.env go test"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@babel/plugin-transform-class-properties": "^7.23.3",
        "@babel/plugin-transform-export-namespace-from": "^7.23.4",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "5.14.18",
        "@mui/lab": "5.0.0-alpha.153",
        "@mui/material": "5.14.18",
        "@mui/styles": "5.14.18",
        "@tanstack/react-location": "^3.7.4",
        "core-js": "^3.33.3",
        "dayjs": "^1.11.10",
        "highcharts": "^11.2.0",
        "highcharts-react-official": "^3.2.1",
        "immer": "^10.0.3",
        "isomorphic-unfetch": "^4.0.2",
        "ramda": "^0.29.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-jsx-highcharts": "^5.0.1",
        "react-perfect-scrollbar": "^1.5.8",
        "react-virtuoso": "^4.6.2",
        "use-async-effect": "^2.2.7",
        "zustand": "4.4.7"
    },
    "devDependencies": {
        "@babel/plugin-proposal-decorators": "^7.23.3",
        "@babel/plugin-proposal-export-default-from": "^7.23.3",
        "@babel/plugin-syntax-import-meta": "^7.10.4",
        "@babel/plugin-transform-async-to-generator": "^7.23.3",
        "@babel/plugin-transform-react-jsx": "^7.23.4",
        "@babel/plugin-transform-regenerator": "^7.23.3",
        "@babel/preset-env": "^7.23.3",
        "@babel/preset-react": "^7.23.3",
        "@babel/preset-typescript": "^7.23.3",
        "@emotion/babel-plugin": "^11.11.0",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
        "@tanstack/react-location-devtools": "^3.4.4",
        "babel-core": "^6.26.3",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.1.3",
        "babel-plugin-dynamic-import-node": "^2.3.3",
        "babel-plugin-jsx-control-statements": "^4.1.2",
        "babel-plugin-styled-components": "^2.1.4",
        "cross-env": "^7.0.3",
        "css-loader": "^6.8.1",
        "enzyme": "^3.11.0",
        "file-loader": "^6.2.0",
        "git-revision-webpack-plugin": "^5.0.0",
        "handlebars": "^4.7.8",
        "handlebars-loader": "^1.7.3",
        "html-webpack-plugin": "^5.5.3",
        "jest": "^29.7.0",
        "jsx-control-statements": "^3.2.8",
        "nodemon": "^3.0.1",
        "npm-run": "^5.0.1",
        "npm-run-all": "^4.1.5",
        "npm-watch": "^0.11.0",
        "react-json-view": "^1.21.3",
        "react-refresh": "^0.14.0",
        "react-test-renderer": "^18.2.0",
        "typescript": "^5.3.2",
        "url-loader": "^4.1.1",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1",
        "webpack-event-plugin": "^1.1.1"
    }
}
